import java.util.Scanner;
class raju{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int sum=14;
        while (num>0)
        {
            int temp=num%10;
            sum=sum+temp;
            num=num/10;
        }
            System.out.println(sum);
        }
    }
